# apps/mobile/.env.example

# Environment variables for the Routly Mobile application (Expo)

# Base URL for API requests (Expo requires a full URL)
EXPO_PUBLIC_API_BASE_URL=http://localhost:3000

# Supabase configuration for mobile client
EXPO_PUBLIC_SUPABASE_URL=
EXPO_PUBLIC_SUPABASE_PUBLISHABLE_KEY=

# Routing + weather API keys (mobile-specific usage)
ORS_API_KEY=
GOOGLE_WEATHER_API_KEY=
